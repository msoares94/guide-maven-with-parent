<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">

  <modelVersion>4.0.0</modelVersion>
 
  <groupId>com.redhat.guide-maven-with-parent</groupId>
  <artifactId>my-parent</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  <packaging>pom</packaging>
 
  <name>My Parent</name>
  <url>https://maven.apache.org</url>
 
  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <junit.version>3.8.1</junit.version>
  </properties>
 
  <dependencies>   
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>${junit.version}</version>
      <scope>test</scope>
    </dependency>
  </dependencies>
  <distributionManagement>
      <repository>
          <id>maven-releases</id>
          <name>Maven Releases Repository</name>
          <url>https://nexustestroute01-nexus-test.apps.ocp4.ibge.gov.br/repository/maven-releases/</url>
      </repository>
      <snapshotRepository>
          <id>maven-snapshots</id>
          <name>Maven Snapshots Repository</name>
          <url>https://nexustestroute01-nexus-test.apps.ocp4.ibge.gov.br/repository/maven-snapshots/</url>
      </snapshotRepository>
  </distributionManagement>
</project>
